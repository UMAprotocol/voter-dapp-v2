{
  "functions": {
    "pages/api/augment-request.ts": {
      "maxDuration": 300
    },
    "pages/api/update-summary.ts": {
      "maxDuration": 300
    },
    "pages/api/cron/warm-discord-threads.ts": {
      "maxDuration": 300
    },
    "pages/api/cron/warm-summaries.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "geolocation=(self), microphone=()"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/warm-summaries",
      "schedule": "*/20 * * * *"
    },
    {
      "path": "/api/cron/warm-discord-threads",
      "schedule": "*/20 * * * *"
    }
  ]
}
